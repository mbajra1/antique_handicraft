<p id="notice"><%= notice %></p>

<h1>Listing State Taxes</h1>

<table>
  <thead>
    <tr>
      <th>State code</th>
      <th>Tax rate</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @state_taxes.each do |state_tax| %>
      <tr>
        <td><%= state_tax.state_code %></td>
        <td><%= state_tax.tax_rate %></td>
        <td><%= link_to 'Show', state_tax %></td>
        <td><%= link_to 'Edit', edit_state_tax_path(state_tax) %></td>
        <td><%= link_to 'Destroy', state_tax, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New State tax', new_state_tax_path %>
