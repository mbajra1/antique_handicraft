<p id="notice"><%= notice %></p>

<h1>Listing Sellers</h1>

<table>
  <thead>
    <tr>
      <th>Seller</th>
      <th>Company</th>
      <th>Last name</th>
      <th>First name</th>
      <th>Email</th>
      <th>Business phone</th>
      <th>Home phone</th>
      <th>Cell phone</th>
      <th>Fax</th>
      <th>Address</th>
      <th>City</th>
      <th>State province</th>
      <th>Zip postal code</th>
      <th>Country region</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sellers.each do |seller| %>
      <tr>
        <td><%= seller.seller_id %></td>
        <td><%= seller.company %></td>
        <td><%= seller.last_name %></td>
        <td><%= seller.first_name %></td>
        <td><%= seller.email %></td>
        <td><%= seller.business_phone %></td>
        <td><%= seller.home_phone %></td>
        <td><%= seller.cell_phone %></td>
        <td><%= seller.fax %></td>
        <td><%= seller.address %></td>
        <td><%= seller.city %></td>
        <td><%= seller.state_province %></td>
        <td><%= seller.zip_postal_code %></td>
        <td><%= seller.country_region %></td>
        <td><%= link_to 'Show', seller %></td>
        <td><%= link_to 'Edit', edit_seller_path(seller) %></td>
        <td><%= link_to 'Destroy', seller, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Seller', new_seller_path %>
